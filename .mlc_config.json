{
  "ignorePatterns": [
    {
      "pattern": "://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/(.*)",
      "replacement": "https://ioos.github.io/$1"
    },
    {
      "pattern": "^\\s*{{\\s*site.baseurl\\s*}}\\s*/(.*)",
      "replacement": "https://ioos.github.io/$1"
    }
],
  "aliveStatusCodes": [200, 400, 403, 0]
}
